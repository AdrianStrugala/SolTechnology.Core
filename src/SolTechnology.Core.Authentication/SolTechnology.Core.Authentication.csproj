<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <Version>0.2.0</Version>
	  <Description>SolTechnology Core Authentication library</Description>
	  <PackageTags>sol technology core azure api authentication</PackageTags>
	  <Product>SolTechnology.Core.Authentication</Product>
	  <PackageId>SolTechnology.Core.Authentication</PackageId>
	  <AssemblyName>SolTechnology.Core.Authentication</AssemblyName>
	  <PackageIcon>docs\mini-logo.png</PackageIcon>
	  <PackageReadmeFile>docs\readme.md</PackageReadmeFile>
	  <!-- NU1510: Packages from Directory.Build.props are already included in FrameworkReference -->
	  <NoWarn>$(NoWarn);NU1510</NoWarn>
  </PropertyGroup>
	<ItemGroup>
		<None Include="..\..\docs\mini-logo.png" Pack="true" Visible="false" PackagePath="docs\mini-logo.png" />
		<None Include="..\..\docs\Auth.md" Pack="true" Visible="false" PackagePath="docs\readme.md" />
	</ItemGroup>
	<ItemGroup>
	  <FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>
	
</Project>
