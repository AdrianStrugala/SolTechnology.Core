<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<Authors>Adrian Strugala</Authors>
		<Version>0.1.0</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Description>SolTechnology Core Azure guards library</Description>
		<PackageTags>sol technology core azure guards</PackageTags>
		<Product>SolTechnology.Core.Guards</Product>
		<PackageId>SolTechnology.Core.Guards</PackageId>
		<RepositoryUrl>https://github.com/AdrianStrugala/SolTechnology.Core</RepositoryUrl>
		<AssemblyName>SolTechnology.Core.Guards</AssemblyName>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>

		<PackageIcon>docs\mini-logo.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\docs\mini-logo.png" Pack="true" Visible="false" PackagePath="docs\mini-logo.png" />
	</ItemGroup>


</Project>
